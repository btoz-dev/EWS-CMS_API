        'WtrNoGenang' => (WATER LOGGING - Tidak ada genangan),
        'WtrParit' => (WATER LOGGING - Ada parit),
        'SucKenaTtkTumbuh' => (SUCKER - Kena titik tumbuh),
        'SucKenaLbgTimbun' => (SUCKER - Lubah tertimbun),
        'SucJmlSes' => (SUCKER - Jumlah sesuai),
        'SucTngSucPlh' => (SUCKER - Tinggi sucker yang dipilih),
        'DelNoDaunSkt' => (DELEAFING - Tidak ada daun sakit),
        'IriSlngKnnKiri' => (IRIGASI - Selang kanan kiri),
        'IriSlngTdkBocor' => (IRIGASI - Selang tidak bocor),
        'IriSlngTdkSumbat' => (IRIGASI - Selang tidak tersumbat),
        'IriTnhLmbb' => (IRIGASI - Tanah Lembab),
        'IriNoDaunPth' => (IRIGASI - Tidak ada daun patah/backlink),
        'HamNoTrace' => (HAMA - Tidak ada bekas serangan hama/penyakit),
        'PpkRata' => (PEMUPUKAN - Rata),
        'PpkAtasSerasah' => (PEMUPUKAN - Tidak di atas serasah),
        'PpkTdkKenaDaun' => (PEMUPUKAN - Tidak terkena daun),
        'PpkJrkTepat' => (PEMUPUKAN - Jarak tepat),
        'KrdlNoKrdl' => (KERDIL - Tanaman tidak kerdil),
        'HillAkarTutup' => (HILLING UP - Akar tanaman tertutup),
        'BITepatWaktu' => (BI - Tepat waktu),
        'BITepatPosisi' => (BI - Tepat posisi),
        'BagTepatWaktu' => (BAGGING - Tepat waktu),
        'BagEmptPlhDrHand1' => (BAGGING - 40 cm dari hand 1),
        'BagDiWiru' => (BAGGING - Di wiru),
        'PrpCkpDalam' => (PROPPING - Cukup kedalaman),
        'PrpTepatPosisi' => (PROPPING - Tepat posisi),
        'PrpTaliKencang' => (PROPPING - Penalian kencang),
        'MrkTepatWaktu' => (MARKING - Tepat waktu),
        'MrkTulisSesuai' => (MARKING - Tulisan sesuai aktual),
        'MrkJmlHand' => (MARKING - Jumlah hand),
        'ForTdkAdaGanggu' => (FOR - Tandan tidak ada gangguan),
        'Notes' => $req->Notes,
        'userid' => (id user),
        'tanggal' => (tanggal dibuatnya),
        'waktu' => (waktu dibuatnya)
    }
]

