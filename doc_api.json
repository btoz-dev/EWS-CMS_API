http://36.66.74.7/api/app/storeBeratTandan
=> INSERT DATA PACKING HOUSE (Berat Tandan)
method = POST*
body = [
    {
        'SubBlkHillCode' => (code tanaman),
        'EmpCode' => (code tukang),
        'BeratBruto' => (berat bruto),
        'BeratBonggol' => (berat bonggol(boleh kosong)),
        'Notes' => (catatan (boleh kosong)),
        'userid' => (id user),
        'tanggal' => (tanggal dibuatnya),
        'waktu' => (waktu dibuatnya)
    }
]

